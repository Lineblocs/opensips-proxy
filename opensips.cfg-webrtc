    if (isflagset("DST_WS")) {
        # INVITE from Web (Caller) -> Sending to CUCM (Callee)
        # We need standard RTP/AVP for CUCM
        $var(reflags) = "replace-origin replace-session-connection rtcp-mux-demux ICE=force media-address=3.97.186.67 SDES=off DTLS=passive UDP/TLS/RTP/SAVPF";
	rtpengine_use_set(1);
    }
    # Otherwise, it is from CUCM/SIP (CUCM -> Web)
    else {

	$var(reflags) = "replace-origin media-address=3.97.186.67 ICE=remove RTP/AVP";
	rtpengine_use_set(1);
	 }
    	rtpengine_offer("$var(reflags)");
}

