route[RTPENGINE_OFFER_1] {
    rtpengine_use_set(1);

    # Default: plain SIP
    $var(flags) = "replace-origin replace-session-connection";

    # If either side of this dialog is WS, treat SDP as WebRTC for this leg
    if (isflagset("SRC_WS") || isflagset("DST_WS")) {
        # WebRTC leg
        $var(flags) = "replace-origin replace-session-connection ICE=force rtcp-mux-require DTLS=passive SDES-off media-address=" + $var(G_PUBLIC_IP);
    }

    xlog("L_INFO", "RTPENGINE_OFFER using flags: $var(flags)\n");
    rtpengine_offer("$var(flags)");
}

route[RTPENGINE_OFFER] {

    $var(rtp_flags) = "";

    # 1. DESTINATION is Web (DST_WS)
    if (isflagset("DST_WS")) {

        if (isflagset("SRC_WS")) {
            # Web --> Web
            $var(rtp_flags) = "generate-mid media-address="+ $var(G_PUBLIC_IP) +" DTLS=passive SDES-off ICE=force UDP/TLS/RTP/SAVPF";
        } else {
            # SIP --> Web
            $var(rtp_flags) = "rtcp-mux-offer generate-mid media-address="+ $var(G_PUBLIC_IP) +" DTLS=passive SDES-off ICE=force UDP/TLS/RTP/SAVPF";
        }

    }
    # 2. DESTINATION is SIP
    else {

        if (isflagset("SRC_WS")) {
            # Web --> SIP
            $var(rtp_flags) = "rtcp-mux-demux DTLS=off SDES-off media-address="+ $var(G_PUBLIC_IP) +" ICE=remove RTP/AVP";
        }
        # Else: SIP --> SIP (Default, no special flags)
    }
	rtpengine_use_set(1);
    $var(rtp_flags) = "replace-origin replace-session-connection " + $var(rtp_flags);
    rtpengine_offer("$var(rtp_flags)");
}

route[RTPENGINE_ANSWER_1] {
    rtpengine_use_set(1);

    # Default: plain SIP
    $var(flags) = "replace-origin replace-session-connection";

    # If this dialog involves WS anywhere, keep it WebRTC-compatible
    if (isflagset("SRC_WS") || isflagset("DST_WS")) {
        $var(flags) = "replace-origin replace-session-connection ICE=force rtcp-mux-require \
                       DTLS=passive SDES-off media-address="+ $var(G_PUBLIC_IP);
    }

    xlog("L_INFO", "RTPENGINE_ANSWER using flags: $var(flags)\n");
    rtpengine_answer("$var(flags)");
}
route[RTPENGINE_ANSWER] {

    # 1. Initialize logic
    $var(reflags) = "";

    # 2. Check: WHO is this Answer going to? (The Original Caller)
    if (isflagset("SRC_WS")) {

        # --- REPLY GOING TO WEB CALLER ---
        # The answer must be WebRTC compatible (SAVPF, ICE, Encrypted)

        if (isflagset("DST_WS")) {
             # Scenario: Web Caller <--- Web Callee
             # Logic: Pass-through / Basic WebRTC params
             $var(reflags) = "generate-mid DTLS=passive SDES-off ICE=force media-address="+ $var(G_PUBLIC_IP) +" UDP/TLS/RTP/SAVPF";
        } else {
             # Scenario: Web Caller <--- SIP Callee
             # Logic: The SIP side sent RTP/AVP. We must ENCRYPT it for the Web Caller.
             # We use 'rtcp-mux-require' to force multiplexing for the browser.
             $var(reflags) = "rtcp-mux-require generate-mid DTLS=passive SDES-off ICE=force media-address="+ $var(G_PUBLIC_IP) +" UDP/TLS/RTP/SAVPF";
        }

    } else {

        # --- REPLY GOING TO SIP CALLER ---
        # The answer must be Standard SIP compatible (RTP/AVP, No ICE)

        if (isflagset("DST_WS")) {
             # Scenario: SIP Caller <--- Web Callee
             # Logic: The Web side sent SAVPF/ICE. We must DECRYPT it for the SIP Caller.
             # We use 'rtcp-mux-demux' to split the RTCP/RTP streams for the SIP gateway.
             $var(reflags) = "rtcp-mux-demux DTLS=off SDES-off ICE=remove media-address="+ $var(G_PUBLIC_IP) +" RTP/AVP";
        }
        # else {
             # Scenario: SIP Caller <--- SIP Callee
             # Standard bridging. Usually empty flags or just 'replace-origin'.
        # }
    }

    # 3. Add Common Flags
    $var(reflags) = "replace-origin replace-session-connection " + $var(reflags);
	rtpengine_use_set(1);
    # 4. Execute
    xlog("L_INFO", "RTP Answer Flags: $var(reflags)\n");
    rtpengine_answer("$var(reflags)");
}

